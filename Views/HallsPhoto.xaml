<UserControl x:Class="PhotoStudioApp.Views.HallsPhoto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PhotoStudioApp.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <Border CornerRadius="20" Background="White">
        <Grid >

            <Image x:Name="image" Grid.Row="0" Stretch="Fill" Cursor="Hand" Width="330" Margin="0 10" Source="/Resources/Pictures/noImages.png"/>

        </Grid>
    </Border>
</UserControl>
